{
  "db_name": "PostgreSQL",
  "query": "\n            SELECT \n                d.id,\n                d.title,\n                d.type as document_type,\n                d.published_at,\n                d.updated_at,\n                u.username as owner_username,\n                u.name as owner_name\n            FROM documents d\n            JOIN users u ON u.id = d.owner_id\n            WHERE d.visibility = 'public' \n            AND d.id = $1 \n            AND u.username = $2\n            ",
